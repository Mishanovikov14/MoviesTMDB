{
  "cli": {
    "version": ">= 14.3.1",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "preview-apk": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "applicationArchivePath": "android/app/build/outputs/apk/release/app-release.apk",
        "buildType": "apk"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "preview4": {
      "distribution": "internal"
    },
    "production": {}
  }
}
